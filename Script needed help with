plr.Chatted:connect(function(c)
	if o1.PrimaryPart ~= nil and fly == true then
		if string.lower(string.sub(c,1,3)) == "/e " then
			cMsg(string.sub(c,4,#c))
		else
			cMsg(c)
		end
	end
end)
